{{> _head }}

<div class="c-canvas jsCanvasContainer">
  <div class="c-canvas__off-canvas jsOffCanvas">
    Mobile Menu located here, but not styled yet
  </div>
  <div class="c-canvas__on-canvas jsOnCanvas">

    {{> _header-main }}

    {{> _hero }}


    {{> _events-header }}
    <div class="l-content">
      {{> _events }}
    </div>

    <div class="c-sponsors--home l-sponsors c-interrupt-2">
      <div class="wrap">
        <h1 class="o-italic">The Sponsors</h1>

        <h3 class="c-event__title">Main Sponsor</h3>
        <img src="images/microsoft.png" alt="">  

        <h3 class="c-event__title">The Supporters</h3>

        <img src="images/piktochart.png" alt="">  
        <img src="images/meteor.png" alt="">  
        <img src="images/paypal.png" alt="">  

        <p> Interested to be part of Devfest? 
          <br>
          <a href="mailto:sponsors@devfest.asia?subject=Devfest Sponsorship" class="o-btn">Be a sponsor!</a>
        </p>

      </div>
    </div>

    {{> _footer }}


  </div>

  {{> _foot }}

